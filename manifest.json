{
  "name": "BrowserUnderControll",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Control websites with an API",
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}